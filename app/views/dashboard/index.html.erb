<% if user_signed_in? %>
  <h1>Dashboard</h1>
    <%= render 'mentries/form', mentry: @mentry %>
    <div id="pie-chart">
      <%= render 'dashboard/piechart' %>
    </div>
    <%= render 'dashboard/maccount_balances' %>
  <h2>Spending Breakdown</h2>
    <%= render 'dashboard/spending' %>
  <% else %>
    <%= render 'loggedout' %>
  <% end %>
