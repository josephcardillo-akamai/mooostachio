<p id="notice"><%= notice %></p>

<h1>Locations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mlocations.each do |mlocation| %>
      <tr>
        <td><%= mlocation.name %></td>
        <td><%= link_to 'Show', mlocation %></td>
        <td><%= link_to 'Edit', edit_mlocation_path(mlocation) %></td>
        <td><%= link_to 'Destroy', mlocation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mlocation', new_mlocation_path %>
